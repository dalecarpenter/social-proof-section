{
  "name": "social-proof-section",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass src/sass/main.scss public/css/styles.css -w",
    "devserver": "live-server --open=/public",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass src/sass/main.scss public/css/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" public/css/styles.comp.css -o public/css/styles.prefix.css",
    "compress:css": "node-sass public/css/styles.prefix.css public/css/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.0.0"
  }
}
